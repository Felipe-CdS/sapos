<%
  form_field = field.object.form_field
  partial_locals = {
    form_field: form_field, field: field,
    configuration: form_field.config_hash,
    filled_class: filled_class
  }

%>

<%= field.hidden_field :form_field_id, class: filled_class %>
<%= field.hidden_field :id, class: filled_class %>
<% if form_field.field_type == Admissions::FormField::STRING %>
  <%= render partial: "admissions/filled_form/edit/generic_string_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::NUMBER %>
  <%= render partial: "admissions/filled_form/edit/generic_number_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::DATE %>
  <%= render partial: "admissions/filled_form/edit/generic_date_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::SELECT %>
  <%= render partial: "admissions/filled_form/edit/generic_select_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::RADIO %>
  <%= render partial: "admissions/filled_form/edit/generic_radio_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::COLLECTION_CHECKBOX %>
  <%= render partial: "admissions/filled_form/edit/generic_collection_checkbox_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::SINGLE_CHECKBOX %>
  <%= render partial: "admissions/filled_form/edit/generic_single_checkbox_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::FILE %>
  <%= render partial: "admissions/filled_form/edit/generic_file_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::HTML %>
  <%= render partial: "admissions/filled_form/edit/generic_html_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::TEXT %>
  <%= render partial: "admissions/filled_form/edit/generic_text_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::STUDENT_FIELD %>
  <%= render partial: "admissions/filled_form/edit/generic_student_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::CITY %>
  <%= render partial: "admissions/filled_form/edit/generic_city_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::RESIDENCY %>
  <%= render partial: "admissions/filled_form/edit/generic_residency_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::SCHOLARITY %>
  <%= render partial: "admissions/filled_form/edit/generic_scholarity_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::CODE %>
  <%= render partial: "admissions/filled_form/edit/generic_string_field",
      locals: partial_locals %>
<% elsif form_field.field_type == Admissions::FormField::EMAIL %>
  <%= render partial: "admissions/filled_form/edit/generic_text_field",
      locals: partial_locals %>
<% end %>
